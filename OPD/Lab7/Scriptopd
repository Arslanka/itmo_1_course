			ORG 	0x012B
TEST1_VAR:	WORD	0x0FFF
TEST1_ANS: 	WORD	0x07FF
TEST2_VAR: 	WORD	0xFFF0
TEST2_ANS: 	WORD	0x7FF8
X1:			WORD	?
X2:			WORD	?
START:		LD 		#0
			ST      ANS
			LD		TEST1_VAR
			WORD	0x0F10
			ST		X1
			CMP		TEST1_ANS
			BNE		EXEC_ERROR
			LD		TEST2_VAR
			WORD	0x0F10
			ST		X2
			CMP		TEST2_ANS
			BNE		EXEC_ERROR
			JUMP 	SUCCESS
EXEC_ERROR:	HLT
SUCCESS:	LD 		#1
			ST      ANS
			HLT
ANS:		WORD	0